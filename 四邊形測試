#include <stdio.h>

int main()
{
	int a,b,c,d;
	printf("請輸入四邊長:");
	scanf("%d %d %d %d",&a,&b,&c,&d);
	if(a+b==2*a&&a+c==2*a&&a+d==2*a)
	{
		printf("菱形");
	}
	else if(a==c&&b==d&&a!=b)
	{
		printf("平行四邊形");
	}
	else if(a+b+c>d&&a+c+d>b&&a+b+d>c&&b+c+d>a)
	{
		printf("其他四邊形");
	}
	else
	printf("BANANA!");

	return 0;
}
